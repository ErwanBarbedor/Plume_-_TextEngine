// Test 'simple eval'
#{1+1}
// Result
2
// End

// Test 'empty var'
#a
// Result

// End

// Test 'eval and set'
\set a {1+1}#a
// Result
2
// End

// Test 'var'
\set a 4 #a
// Result
 4
// End

// Test 'type 2'
\set a 2#{type(a)}
// Result
number
// End

// Test 'type 3'
\set a {2}
#{type(a)}
// Result

number
// End

// Test 'type 4'
\set a {\{\}}#{type(a)}
// Result
table
// End

// Test 'type 5'
\set a {{}}#{type(a)}
// Result
table
// End

// Test 'type 6'
\set a {function (x) return 2*x end}
#{a(5)}
// Result

10
// End

// Test 'Capture 1'
\set a {10}#a+1
// Result
10+1
// End

// Test 'Capture 2'
\set a {10}#{a+1}
// Result
11
// End

// Test 'Eval table'
#{ ({1, 2})[1] }
// Result
1
// End

// Test 'Access _G'
\set a 1 //
#{_G.a}
// Result
 1
// End

