// Test 'simple eval'
#{1+1}
// Result
2
// End

// Test 'empty var'
#a
// Result

// End

// Test 'eval and set'
#{a = 1+1}#a
// Result
2
// End

// Test 'var'
#{a = 4} #a
// Result
 4
// End

// Test 'type 2'
#{a = 2}#{type(a)}
// Result
number
// End

// Test 'type 3'
#{a = 2}
#{type(a)}
// Result

number
// End

// Test 'type 4'
#{a={}}#{type(a)}
// Result
table
// End

// Test 'type 6'
#{a = function (x) return 2*x end}
#{a(5)}
// Result

10
// End

// Test 'Capture 1'
#{a = 10}#a+1
// Result
10+1
// End

// Test 'Capture 2'
#{a = 10}#{a+1}
// Result
11
// End

// Test 'Eval table'
#{ ({1, 2})[1] }
// Result
1
// End

// Test 'Format eval'
#{1/3}[.2f]
// Result
0.33
// End

